	<Field name="Txt" type="java.lang.String"><![CDATA[from java.lang import Class 

from com.essbase.api.session import IEssbase
from com.essbase.api.domain import IEssDomain
from com.essbase.api.datasource import IEssCube
from com.essbase.api.datasource import IEssOlapServer
from com.essbase.api.datasource import IEssOlapApplication
from com.essbase.api.base import IEssIterator
from com.essbase.api.base import EssException

from java.sql import DriverManager 
from java.util import Properties  

from time import clock as now  

# Target (Destination) Connection
conTrg = snpRef.getJDBCConnection( "DEST" )   

#
# Source essbase connection properties
#
s_olapSvrName =  "<%=snpRef.getInfo("SRC_DSERV_NAME")%>"
s_userName    = "<%=snpRef.getInfo("SRC_USER_NAME")%>"
s_password   = "<%=snpRef.getInfo("SRC_PASS")%>"

s_provider = "<%=snpRef.getOption("PROVIDER_URL")%>" 

srvportParts = s_olapSvrName.split(':',2)
s_olapSvrName =  srvportParts[0]

ess = None
variables = []
try:
    # Create JAPI instance.
    ess = IEssbase.Home.create(IEssbase.JAPI_VERSION)

    # Sign On to the Provider
    dom = ess.signOn(s_userName, s_password, 0, None, s_provider);
    olapSvr = dom.getOlapServer(s_olapSvrName);
    olapSvr.connect();

    # Add Local variables
    olapApps = olapSvr.getApplications();
    for i in range(olapApps.getCount()): 
        app = olapApps.getAt(i);
        appVar = app.getSubstitutionVariables();
